
// Import the File class
import java.io.File;
 
public class day3 {
    public static void main(String[] args)
    {
        File Obj = new File("yash.txt");
        if (Obj.delete()) {
            System.out.println("The deleted file is : "
                               + Obj.getName());
        }
        else {
            System.out.println(
                "Failed in deleting the file.");
        }
    }
}
