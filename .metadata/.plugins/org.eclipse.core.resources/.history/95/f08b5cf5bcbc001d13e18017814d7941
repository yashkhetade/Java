package mypack2;

import java.awt.*;

import java.awt.event.*;




public class Jdemo {
	
	
	 Frame f=new Frame("Mouse Events");
	Jdemo(){
		
		
		
		
		f.setLayout(new FlowLayout());
		
		f.addWindowListener(new WindowAdapter(){
		public void windowClosing (WindowEvent e) {
			f.dispose();
			
		}
		
		});
		
		
		f.setSize(500,500);
		f.setVisible(true);
		
		
		
	}
	
	

public static void main(String[] args) {
	
	Jdemo obj=new Jdemo();
	
}
	
	
}
