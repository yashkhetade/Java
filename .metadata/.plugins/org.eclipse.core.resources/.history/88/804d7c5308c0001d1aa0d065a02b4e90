package mypack;

import java.util.*;


public class Stack5 {
	
	public static boolean checkpalindrome(String str) {
		
		
		
		
	}
	
	public static int length(String str) {
		
		int count=1;
		for(int i=0;i!='\0';i++) {
			count++;
			
		}
		return count;
		
		
	}

	public static void main(String[] args) {
		
		
		String str="yash";
		
		System.out.println(length(str));
		
		
	}
}
