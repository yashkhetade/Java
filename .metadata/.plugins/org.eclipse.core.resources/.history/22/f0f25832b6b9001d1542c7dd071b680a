package mypack4;


class Animal{
	
	void show() {
		System.out.println("i am parent class method ");
		
	}
	
}


class dog extends Animal{
	
	void display() {
		System.out.println("i am dog ");
		
	}
}







public class SIngle {

	
	public static void main(String[] args) {
		
		
		
		dog obj2=new dog();
		obj2.display();
		obj2.show();
		
		
	}
}
